###########################################################
# Do NOT modify this file, make a copy and rename to .env #
###########################################################

# Project name for the docker-compose file
# Don't need to change this
COMPOSE_PROJECT_NAME=cosmos

# Fill in missing vars after copying or docker-compose up will fail
# Influxdb initial configuration
# Admin user
INFLUXDB_INIT_ADMIN_USERNAME=
INFLUXDB_INIT_ADMIN_PASSWORD=
# Organization name
INFLUXDB_INIT_ORG=initorg
# Bucket name
# Some long string used to authenticate access to bucket
INFLUXDB_INIT_ADMIN_TOKEN=INFLUXDBINITADMINTOKEN

# Grafana initial configuration
GF_ADMIN_USER=
GF_ADMIN_PASSWORD=
GF_APP_MODE=development